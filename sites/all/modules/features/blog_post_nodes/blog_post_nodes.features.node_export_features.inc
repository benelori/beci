      \'title\' => \'Pénzügy középiskolásoknak\',
      \'log\' => \'\',
      \'status\' => \'1\',
      \'comment\' => \'1\',
      \'promote\' => \'0\',
      \'sticky\' => \'0\',
      \'vuuid\' => \'02ff1f6f-1196-481a-85c5-f3fd1a0c63f7\',
      \'nid\' => \'732\',
