      \'title\' => \'II. hely a pécsi esettanulmány versenyen 2014-ben\',
      \'log\' => \'\',
      \'status\' => \'1\',
      \'comment\' => \'1\',
      \'promote\' => \'0\',
      \'sticky\' => \'0\',
      \'vuuid\' => \'6ec605ae-9379-4960-ada9-29bdcb47f0a5\',
