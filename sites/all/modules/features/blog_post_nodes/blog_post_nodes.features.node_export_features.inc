  (object) array(
      \'vid\' => \'738\',
      \'uid\' => \'3\',
      \'title\' => \'Döntőben a stratégia esettanulmány-versenyen\',
      \'log\' => \'\',
      \'status\' => \'1\',
      \'comment\' => \'1\',
      \'promote\' => \'0\',
      \'sticky\' => \'0\',
      \'vuuid\' => \'ffe575ca-4630-4699-a928-9a09d2c51a6b\',
