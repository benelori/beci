      \'vuuid\' => \'61365f75-c605-4fe1-a323-2f5fc38eb0a2\',
      \'nid\' => \'746\',
      \'type\' => \'gtk_blog_post\',
      \'language\' => \'hu\',
      \'created\' => \'1405544400\',
