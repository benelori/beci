      \'sticky\' => \'0\',
      \'vuuid\' => \'8db564e2-ef1c-45fa-bbc4-3ccd3929a759\',
      \'nid\' => \'730\',
      \'type\' => \'gtk_blog_post\',
      \'language\' => \'hu\',
      \'created\' => \'1418853600\',
