      \'comment\' => \'1\',
      \'promote\' => \'0\',
      \'sticky\' => \'0\',
      \'vuuid\' => \'624476fa-e5cb-46b0-a8aa-ce3a22e270ce\',
      \'nid\' => \'782\',
      \'type\' => \'gtk_blog_post\',
      \'language\' => \'hu\',
      \'created\' => \'1386885600\',
