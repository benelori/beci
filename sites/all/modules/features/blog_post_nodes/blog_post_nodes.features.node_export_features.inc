      \'vid\' => \'767\',
      \'uid\' => \'3\',
      \'title\' => \'Részvétel a XV. Országos Esettanulmány Versenyen\',
      \'log\' => \'\',
      \'status\' => \'1\',
      \'comment\' => \'1\',
      \'promote\' => \'0\',
      \'sticky\' => \'0\',
      \'vuuid\' => \'580db69c-5f8d-4b3b-bb94-66d011da26a7\',
      \'nid\' => \'767\',
      \'type\' => \'gtk_blog_post\',
      \'language\' => \'hu\',
      \'created\' => \'1394402400\',
