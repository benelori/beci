      \'title\' => \'Mummert Alapítvány ösztöndíjában részesült\',
      \'log\' => \'\',
      \'status\' => \'1\',
      \'comment\' => \'1\',
      \'promote\' => \'0\',
      \'sticky\' => \'0\',
      \'vuuid\' => \'d4c52b92-ef5d-4f8b-a0db-e4a21e10f1c9\',
      \'nid\' => \'770\',
      \'type\' => \'gtk_blog_post\',
